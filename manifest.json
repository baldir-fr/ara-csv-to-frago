{
    "manifest_version": 2,
 
    "name": "Tab Dump to Markdown", 
    "description": "My first Chrome extension.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab","clipboardWrite", "tabCapture", "tabs"
     ]
}