{
    "name": "Tab Dump to Markdown",
    "version": "1.0",
    "manifest_version": 3,

    "author": "Marc Bouvier",
    "description": "Dumps pages opened in tabs as markdown list",
    "default_locale": "en",
    "homepage_url":"",
    
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup/index.html"
    },
    "permissions": [
        "tabs",
        "clipboardWrite"
    ]
}